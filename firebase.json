{
  
  "hosting": {
    
    
    "public": "apps/qwik/dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "server"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
